reserved-memory {
	#address-cells = <2>;
	#size-cells = <2>;

	elog {
		reg = <0x0 0x8f130000 0x0 0x10000>;
	};
};

idm {
	compatible = "simple-bus";
	dma-ranges;
	ranges = <0x0 0x0 0x0 0xffffffff>;
	#address-cells = <1>;
	#size-cells = <1>;

	idm_elog: idm-elog {
		compatible = "brcm,iproc-idm", "brcm,iproc-idm-elog";
		reg = <0x8f130000 0x10000>;
	};

	idm-paxb0-axi {
		compatible = "brcm,iproc-idm", "brcm,sr-idm-paxb0-axi";
		reg = <0x58000900 0x200>;
		interrupts = <GIC_SPI 473 IRQ_TYPE_LEVEL_HIGH>;
		brcm,iproc-idm-elog = <&idm_elog>;
	};

